<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@color/acquiesce"
    tools:context=".ui.fragment.retire.RetireDetectionkFragment">
    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/pet_me_tb"
        app:navigationIcon="@drawable/ic_back"
        android:background="@color/purple_500">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="推荐机构"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:layout_gravity="center"/>
    </androidx.appcompat.widget.Toolbar>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:background="@color/acquiesce"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <androidx.appcompat.widget.SearchView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/search_bg"
                android:layout_marginTop="@dimen/five"
                android:layout_marginHorizontal="16dp"/>
            <LinearLayout
                android:layout_marginTop="@dimen/ten"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <RelativeLayout
                    android:id="@+id/detet_1"
                    android:layout_marginTop="@dimen/ten"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:background="@drawable/retire_bg"
                    android:layout_marginHorizontal="20dp">
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/light_gray"
                        android:id="@+id/xianA_1"
                        android:layout_marginTop="30dp"/>
                    <ImageView
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:src="@drawable/ic_retire_12"
                        android:layout_alignParentLeft="true"
                        android:layout_centerInParent="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginLeft="40dp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="健康公寓"
                        android:textColor="@color/gray"
                        android:textSize="18sp"
                        android:id="@+id/tuiT1"
                        android:layout_centerInParent="true"/>
                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:src="@drawable/ic_retire_13"
                        android:layout_below="@+id/tuiT1"
                        android:layout_marginLeft="150dp"
                        android:id="@+id/din1"
                        android:layout_marginTop="@dimen/ten"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="北京"
                        android:textSize="14dp"
                        android:textColor="@color/light_gray"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="120dp"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/light_gray"
                        android:layout_below="@+id/xianA_1"
                        android:layout_marginTop="140dp"/>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/detet_2"
                    android:layout_marginTop="@dimen/ten"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:background="@drawable/retire_bg"
